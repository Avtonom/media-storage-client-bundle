{
  "name": "avtonom/media-storage-client-bundle",
  "type": "symfony-bundle",
  "description": "Symfony media client bundle for uploading (sending) files and images to a remote server deployed on SonataMediaBundle",
  "keywords": ["Symfony2", "bundle", "SonataMediaBundle", "SonataMediaBundleClient", "storage", "MediaStorage", "MediaStorageClient"],
  "homepage": "https://github.com/Avtonom/media-storage-client-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton U",
      "email": "avtonomspb@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "symfony/dependency-injection": "~2.3|3.*",
    "symfony/monolog-bundle": "2.*",
    "sensio/buzz-bundle":"~1.0"
  },
  "autoload": {
    "psr-0": { "Avtonom\\MediaStorageClientBundle": "" }
  },
  "target-dir": "Avtonom/MediaStorageClientBundle",
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  }
}